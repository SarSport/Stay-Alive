<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %} {{ 'brand.name'|trans }} {% endblock %}</title>
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/default/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/default/css/bootstrap-responsive.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/default/css/main.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/default/css/stay-alive.css') }}">
        {% block stylesheets %}{% endblock %}
        {% block favicon %}<link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />{% endblock %}
        <!--[if IE]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
        <script src="{{ asset('bundles/default/js/jquery-v1.7.1.min.js') }}"></script>
        <script src="{{ asset('bundles/default/js/json.js') }}"></script>
        <script src="{{ asset('bundles/default/js/hc_fix.js') }}"></script>

        <script src="{{ asset('bundles/default/js/bootstrap/bootstrap-modal.js') }}"></script>
        <script src="{{ asset('bundles/default/js/modal_focus.js') }}"></script>
        <script src="{{ asset('bundles/default/js/bind_modal_focus.js') }}"></script>
    </head>
    <body>
        {% block body %}
            {% block topbar %}{% include '::topbar.html.twig' %}{% endblock %}
            {% block topbanner %}{% include '::topbanner.html.twig' %}{% endblock %}
            {% block container %}
                <div class="container-fluid">
                    <div class="row-fluid">
                        <div class="span12">
                            <div class="row-fluid">
                                {% block sidebar %}
                                <div class="span2 page-sidebar-left" id="page-sidebar">
                                    {% block leftMenu %}
                                        {% include '::navbar.html.twig' %}
                                    {% endblock %}
                                </div>
                                {% endblock %}
                                <div class="span7 page-content" id="page-content">
                                    <div id="main_column">
                                        {% block flashMessages %}
                                            {% block errorMmessage %}
                                                {% if app.session.flash('error') %}
                                                <div class="alert-message error">
                                                    <p><em>Error</em>: {{ app.session.flash('error') }}</p>
                                                </div>
                                                {% endif %}
                                            {% endblock %}
                                            {% block successMessage %}
                                                {% if app.session.flash('notice') %}
                                                <div class="alert-message success">
                                                    <p><em>Notice</em>: {{ app.session.flash('notice') }}</p>
                                                </div>
                                                {% endif %}
                                            {% endblock %}
                                        {% endblock %}
                                        {% block content %}{% endblock %}
                                    </div>
                                </div>
                                <div class="span3 page-sidebar-right">
                                    <div class="right_category">
                                        <h4>Header</h4>
                                        <ul style="list-style:none;margin:0px;padding:0px;border:0px;">

                                            <li><div class="latest_item latest_quiz">
                                                <a href="#">Some link</a>&nbsp;
                                                <br><span class="due_date">Date</span>
                                            </div></li>
                                        </ul>
                                    </div>

                                    <div class="right_category">
                                        <h4>Header 2</h4>
                                        <ul style="list-style:none;margin:0px;padding:0px;border:0px;">

                                            <li><div class="latest_item latest_assignment">
                                                <a href="#">Some link2</a>&nbsp;
                                                <br><span class="due_date">Date 2</span>
                                            </div></li>
                                        </ul>
                                    </div>

                                    <div class="right_category">
                                        <h4>Header3</h4>
                                        <ul style="list-style:none;margin:0px;padding:0px;border:0px;">

                                            <li><div class="latest_item latest_lecture">
                                                text1&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text2&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text3&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text4&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text5&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text6&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text7&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                3.14 - Forms&nbsp;
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text8
                                            </div></li>

                                            <li><div class="latest_item latest_lecture">
                                                text9&nbsp;
                                            </div></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {% endblock %}
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
